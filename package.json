{
  "version": "3.0.0-beta.39",
  "name": "winnetoujs",
  "description": "The indie javascript constructor",
  "main": "src/winnetou.js",
  "files": [
    "dist/"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/winnetou.d.ts",
      "import": "./dist/core/winnetou.js"
    },
    "./core/constructos": {
      "types": "./dist/core/constructos.d.ts",
      "import": "./dist/core/constructos.js"
    },
    "./core/winnetou": {
      "types": "./dist/core/winnetou.d.ts",
      "import": "./dist/core/winnetou.js"
    },
    "./modules/colorThemes": {
      "types": "./dist/modules/colorThemes.d.ts",
      "import": "./dist/modules/colorThemes.js"
    },
    "./modules/router": {
      "types": "./dist/modules/router.d.ts",
      "import": "./dist/modules/router.js"
    },
    "./modules/select": {
      "types": "./dist/modules/select.d.ts",
      "import": "./dist/modules/select.js"
    },
    "./modules/select/animations": {
      "types": "./dist/modules/select/animations.d.ts",
      "import": "./dist/modules/select/animations.js"
    },
    "./modules/select/attributes": {
      "types": "./dist/modules/select/attributes.d.ts",
      "import": "./dist/modules/select/attributes.js"
    },
    "./modules/select/dom-manipulation": {
      "types": "./dist/modules/select/dom-manipulation.d.ts",
      "import": "./dist/modules/select/dom-manipulation.js"
    },
    "./modules/select/events": {
      "types": "./dist/modules/select/events.d.ts",
      "import": "./dist/modules/select/events.js"
    },
    "./modules/select/index": {
      "types": "./dist/modules/select/index.d.ts",
      "import": "./dist/modules/select/index.js"
    },
    "./modules/select/navigation": {
      "types": "./dist/modules/select/navigation.d.ts",
      "import": "./dist/modules/select/navigation.js"
    },
    "./modules/select/positioning": {
      "types": "./dist/modules/select/positioning.d.ts",
      "import": "./dist/modules/select/positioning.js"
    },
    "./modules/select/selectors": {
      "types": "./dist/modules/select/selectors.d.ts",
      "import": "./dist/modules/select/selectors.js"
    },
    "./modules/translations": {
      "types": "./dist/modules/translations.d.ts",
      "import": "./dist/modules/translations.js"
    },
    "./modules/ssr": {
      "types": "./dist/modules/ssr.d.ts",
      "import": "./dist/modules/ssr.js"
    }
  },
  "dependencies": {
    "@types/node": "^24.0.10",
    "commander": "^14.0.0",
    "esbuild": "^0.25.9",
    "escape-string-regexp": "^4.0.0",
    "js-beautify": "^1.15.4",
    "ncp": "2.0.0",
    "node-html-parser": "^7.0.1",
    "node-watch": "^0.7.4",
    "recursive-readdir": "^2.2.3"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "scripts": {
    "start": "esbuild 'src/**/*.ts' --outdir=dist --format=esm --platform=browser --target=es2020 --sourcemap --outbase=src --resolve-extensions=.ts,.js && cp -r source/* dist/ && npm run build-types",
    "build-fs": "node ./dist/buildWBR.js",
    "postinstall": "npm run-script build-fs",
    "type-check": "tsc --noEmit",
    "build-types": "tsc --emitDeclarationOnly",
    "publish:beta": "npm publish --tag beta",
    "publish:alpha": "npm publish --tag alpha",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedrosdev/winnetoujs.git"
  },
  "keywords": [
    "javascript",
    "winnetou",
    "winnetoujs",
    "win",
    "framework",
    "frontend"
  ],
  "author": "Pamela Sedrez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cedrosdev/winnetoujs/issues"
  },
  "homepage": "https://winnetoujs.org"
}
